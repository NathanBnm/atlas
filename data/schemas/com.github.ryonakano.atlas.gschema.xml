<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/com/github/ryonakano/atlas/" id="com.github.ryonakano.atlas">
		<key type="d" name="langitude">
			<default>0</default>
		</key>
		
		<key type="d" name="longitude">
			<default>0</default>
		</key>
		
		<key type="i" name="zoom-level">
			<default>8</default>
		</key>
		
		<key type="i" name="window-width">
			<default>1000</default>
		</key>
		
		<key type="i" name="window-height">
			<default>800</default>
		</key>
		
		<key type="i" name="position-x">
			<default>-1</default>
		</key>
		
		<key type="i" name="position-y">
			<default>-1</default>
		</key>
		
		<key type="b" name="maximized">
			<default>false</default>
		</key>
    <key name="is-prefer-dark" type="b">
      <default>false</default>
      <summary>Prefer dark</summary>
      <description>Whether to show the window in a dark style or not</description>
    </key>
    <key name="is-follow-system-style" type="b">
      <default>true</default>
      <summary>Follow system style</summary>
      <description>Whether to follow system's dark style settings or not</description>
    </key>
 	</schema>
</schemalist>
